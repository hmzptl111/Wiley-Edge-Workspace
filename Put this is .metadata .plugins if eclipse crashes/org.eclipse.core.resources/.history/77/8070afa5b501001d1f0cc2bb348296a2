package com.sujata.demo;

abstract public class Car {

	private String model;
	private String color;
	public Car(String model, String color) {
		super();
		this.model = model;
		this.color = color;
	}
	public String getModel() {
		return model;
	}
	public String getColor() {
		return color;
	}
	
	public void engine();
}
